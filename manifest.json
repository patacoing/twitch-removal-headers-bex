{
    "manifest_version": 3,
    "name": "Twitch headers removal",
    "description": "Extension to remove the headers from twitch's chat",
    "version": "1.0",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/icon.png"
    },
    "content_scripts": [{
        "matches": ["https://www.twitch.tv/*"],
        "js": [
            "scripts/content.js",
            "utils/removeElement.js"
        ]
    }]
  }